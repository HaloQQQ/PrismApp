<Window
    x:Class="CustomControlsDemoModule.Views._2048Window"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:icetea="http://IceTea.Wpf.Core.com/"
    xmlns:local="clr-namespace:CustomControlsDemoModule.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="_2048Window"
    MinWidth="1200"
    MinHeight="640"
    Style="{StaticResource WithoutTitleBar}"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <DockPanel>
        <icetea:WindowTitleBar DockPanel.Dock="Top" />

        <local:_2048 DockPanel.Dock="Bottom" />
    </DockPanel>
</Window>
