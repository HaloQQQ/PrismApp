<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\apple.ico</ApplicationIcon>
    <Version></Version>
    <AssemblyVersion>1.0.1.3</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="appsettings.json" />
    <None Remove="Resources\apple.ico" />
    <None Remove="Resources\iconfont.ttf" />
    <None Remove="Resources\网络.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\apple.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="IceTea.NetCore" Version="1.0.6" />
    <PackageReference Include="IceTea.SocketStandard" Version="1.0.0" />
    <PackageReference Include="Prism.DryIoc" Version="8.1.97" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MusicPlayerModule\MusicPlayerModule.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Resource Include="Resources\apple.ico" />
    <Resource Include="Resources\iconfont.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\网络.png" />
  </ItemGroup>

</Project>
