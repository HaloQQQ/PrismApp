<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="IconStackPanel.MouseOver.Background" Color="#efefef"/>
    <SolidColorBrush x:Key="IconStackPanel.MouseOver.Foreground" Color="#35c1ff"/>

    <SolidColorBrush x:Key="Container.Background" Color="White"/>

    <SolidColorBrush x:Key="TextBox.Background" Color="#efefef"/>
    
    <SolidColorBrush x:Key="TextBox.Focused.BorderBrush" Color="Blue"/>
    
    <SolidColorBrush x:Key="TextBlock.Foreground" Color="Black"/>
    <SolidColorBrush x:Key="TextBlock.Background" Color="White"/>
    
    
    <SolidColorBrush x:Key="SlideCheckBox.BorderBrush" Color="LightGreen"/>
    
    <Color x:Key="ShadowColor" R="128" G="128" B="128"/>
</ResourceDictionary>